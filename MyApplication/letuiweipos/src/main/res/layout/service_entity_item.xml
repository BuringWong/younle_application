<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40px"
        android:background="#f2f2f2"
        android:id="@+id/al_date_account"
        >
        <TextView
            android:id="@+id/order_date"
            android:layout_width="wrap_content"
            android:layout_height="40px"
            android:textSize="18px"
            android:textColor="#999"
            android:layout_marginLeft="20px"
            android:text="今天"
            android:gravity="center_vertical"
            />
        <TextView
            android:id="@+id/order_day_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20px"
            android:textSize="18px"
            android:textColor="#999"
            android:text="收入：200.00元"
            android:layout_centerVertical="true"
            />
    </com.zhy.autolayout.AutoRelativeLayout>
    <include layout="@layout/entity_order_info"
        android:id="@+id/al_service_entity"/>
    <View
        android:id="@+id/day_depart_line"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#cdcdcd"
        android:visibility="gone"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="10dp"/>
    <com.younle.younle624.myapplication.view.MyListView
        android:id="@+id/gv_sell_finish"
        android:divider="@null"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.younle.younle624.myapplication.view.MyListView>
</com.zhy.autolayout.AutoLinearLayout>