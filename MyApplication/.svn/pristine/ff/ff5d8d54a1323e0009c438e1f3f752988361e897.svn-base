<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="360px"
    android:layout_height="105px"
    >
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_content"
        android:layout_width="wrap_content"
        android:layout_height="105px"
        android:orientation="vertical"
        android:layout_marginTop="22px"
        android:layout_marginLeft="18px">
        <TextView
            android:id="@+id/tv_delete_servicename"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="泰式按摩1小时"
            android:textSize="25px"
            android:textColor="#292929"/>
        <TextView
            android:id="@+id/tv_delete_waitername_leavel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="初级技师：雷军"
            android:textSize="20px"
            android:textColor="#666"
            android:layout_marginTop="16px"
            />

    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:clickable="true"
        android:id="@+id/rl_delete"
        android:layout_width="75px"
        android:layout_height="130px"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:background="@drawable/delete_selector">
        <ImageView
            android:id="@+id/btn_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/minus_little"
            android:layout_centerInParent="true"
            android:textSize="18px"
            android:textColor="#fff"
            />
    </com.zhy.autolayout.AutoRelativeLayout>

    <TextView
        android:id="@+id/tv_count"
        android:layout_width="wrap_content"
        android:layout_height="105px"
        android:text="X2"
        android:textColor="#292929"
        android:textSize="28px"
        android:gravity="center_vertical"
        android:layout_toLeftOf="@id/rl_delete"
        android:layout_marginRight="18px"
        />
    <View
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#f2f2f2"/>
</com.zhy.autolayout.AutoRelativeLayout>