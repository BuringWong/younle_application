<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="606px"
    android:minWidth="606px"
    android:layout_height="390px"
    android:minHeight="390px"
    android:gravity="center"
    android:background="@drawable/pup_shape">
    <TextView
        android:id="@+id/title"
        android:layout_marginTop="40px"
        android:layout_width="360px"
        android:layout_height="0px"
        android:layout_weight="1"
        android:text="款项未退还"
        android:textSize="24px"
        android:textColor="#ff5500"
        android:gravity="center"/>
    <TextView
        android:id="@+id/tv_notice_msg"
        android:layout_width="360px"
        android:layout_height="0px"
        android:layout_weight="4"
        android:text="error"
        android:maxEms="13"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="22px"
        android:textColor="#666"
        android:paddingLeft="20px"
        android:paddingRight="20px"
        android:layout_marginTop="30px"
        android:gravity="center_vertical"/>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginBottom="55px"
        android:layout_marginTop="30px"
        >
        <TextView
            android:id="@+id/tv_no"
            android:layout_width="200px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="暂不退款"
            android:textSize="28px"
            android:background="@drawable/native_btn_selector"/>
        <TextView
            android:id="@+id/tv_yes"
            android:layout_width="200px"
            android:layout_height="match_parent"
            android:textColor="#fff"
            android:text="我已退款"
            android:background="@drawable/positive_btn_selector"
            android:gravity="center"
            android:textSize="28px"
            android:layout_marginLeft="35px"
            />
    </com.zhy.autolayout.AutoLinearLayout>
