<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="606px"
    android:minWidth="606px"
    android:layout_height="390px"
    android:minHeight="390px"
    android:gravity="center_horizontal"
    android:background="@drawable/pup_shape">
    <TextView
        android:id="@+id/tv_notice_msg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请选择打印机类型"
        android:textSize="32px"
        android:paddingLeft="20px"
        android:paddingRight="20px"
        android:textColor="#333"
        android:gravity="center"
        android:layout_marginTop="82px"
        />
    <TextView
        android:id="@+id/tv_yes"
        android:layout_width="330px"
        android:layout_height="80px"
        android:textColor="#fff"
        android:text="蓝牙打印机"
        android:background="@drawable/positive_btn_selector"
        android:gravity="center"
        android:textSize="28px"
        android:layout_marginTop="74px"
        />
    <TextView
        android:id="@+id/tv_no"
        android:layout_width="330px"
        android:layout_height="80px"
        android:textColor="#fff"
        android:text="云打印机"
        android:background="@drawable/positive_btn_selector"
        android:gravity="center"
        android:textSize="28px"
        android:layout_marginTop="36px"
        />

</com.zhy.autolayout.AutoLinearLayout>