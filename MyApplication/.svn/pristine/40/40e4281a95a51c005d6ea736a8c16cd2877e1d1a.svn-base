package com.younle.younle624.myapplication.domain.membercharge;

import java.io.Serializable;

/**
 * 作者：Create by 我是奋斗 on2016/12/26 10:56
 * 邮箱：tt090423@126.com
 * 微信：17346512596
 * 下单充值，会员充值的选项 bean
 */
public class ChargeItem implements Serializable {
    private String monthPay;
    private String seasonPay;
    private String halfYearPay;
    private long yearPay;
    private String whichModular;

    public ChargeItem() {
    }

    public ChargeItem(String monthPay, String seasonPay, String halfYearPay, long yearPay, String whichModular) {
        this.monthPay = monthPay;
        this.seasonPay = seasonPay;
        this.halfYearPay = halfYearPay;
        this.yearPay = yearPay;
        this.whichModular = whichModular;
    }

    public ChargeItem(String whichModular) {
        this.whichModular = whichModular;
    }

    public String getMonthPay() {
        return monthPay;
    }

    public void setMonthPay(String monthPay) {
        this.monthPay = monthPay;
    }

    public String getSeasonPay() {
        return seasonPay;
    }

    public void setSeasonPay(String seasonPay) {
        this.seasonPay = seasonPay;
    }

    public String getHalfYearPay() {
        return halfYearPay;
    }

    public void setHalfYearPay(String halfYearPay) {
        this.halfYearPay = halfYearPay;
    }

    public long getYearPay() {
        return yearPay;
    }

    public void setYearPay(long yearPay) {
        this.yearPay = yearPay;
    }

    public String getWhichModular() {
        return whichModular;
    }

    public void setWhichModular(String whichModular) {
        this.whichModular = whichModular;
    }

    @Override
    public String toString() {
        return "ChargeItem{" +
                "monthPay='" + monthPay + '\'' +
                ", seasonPay='" + seasonPay + '\'' +
                ", halfYearPay='" + halfYearPay + '\'' +
                ", yearPay='" + yearPay + '\'' +
                ", whichModular='" + whichModular + '\'' +
                '}';
    }
}
