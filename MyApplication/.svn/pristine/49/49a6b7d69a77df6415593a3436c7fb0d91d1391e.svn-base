<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="5px"/>
            <stroke android:color="@color/print_normal" android:width="1px"/>
            <solid android:color="@color/print_normal"/>
            <size android:width="130px" android:height="55px"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="5px"/>
            <stroke android:color="@color/print_press" android:width="1px"/>
            <solid android:color="@color/print_press"/>
            <size android:width="130px" android:height="55px"/>
        </shape>
    </item>
</selector>