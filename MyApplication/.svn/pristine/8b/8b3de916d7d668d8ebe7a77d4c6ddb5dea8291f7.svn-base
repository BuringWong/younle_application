/**
 * 请求被扫支付API需要提交的数据
 */
public class ScanPayReqData {
  //每个字段具体的意思请查看API文档
    private String auth_code = "";
    private String out_trade_time = "";
    private String total_fee = "";

    public ScanPayReqData(String auth_code,String out_trade_time,String total_fee) {
        this.auth_code = auth_code;
        this.out_trade_time = out_trade_time;
        this.total_fee = total_fee;
    }

    public String getAuth_code() {
        return auth_code;
    }
    public void setAuth_code(String auth_code) {
        this.auth_code = auth_code;
    }
    public String getOut_trade_time() {
        return out_trade_time;
    }
    public void setOut_trade_time(String out_trade_no) {
        this.out_trade_time = out_trade_time;
    }
    public String getTotal_fee() {
        return total_fee;
    }
    public void setTotal_fee(String total_fee) {
        this.total_fee = total_fee;
    }
    @Override
    public String toString() {
        return "ScanPayReqData{" +
                "auth_code='" + auth_code + '\'' +
                ", out_trade_no='" + out_trade_time + '\'' +
                ", total_fee=" + total_fee +
                '}';
    }
}
