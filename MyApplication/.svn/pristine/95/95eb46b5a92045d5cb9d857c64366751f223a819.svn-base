<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingTop="10dp" >
    <TextView
        android:id="@+id/tv_timedia_title"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="请选择打印数据的时间范围"
        android:textSize="30px"
        android:textColor="#333"
        android:layout_marginTop="50px"
        android:layout_marginBottom="20px"
        android:gravity="center"/>
    <View
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="30px"
        android:layout_marginRight="30px"
        android:layout_marginBottom="10px"
        android:background="#ccc"/>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/start_title"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="开始日期"
            android:layout_centerHorizontal="true"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/start_title"
            android:layout_centerHorizontal="true"
            android:paddingLeft="27px">
            <DatePicker
                android:layout_below="@id/start_title"
                android:theme="@android:style/Theme.Holo.Light.NoActionBar.Overscan"
                android:id="@+id/datePickerStart"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:calendarViewShown="false"
                />
            <TimePicker
                android:id="@+id/tp_start"
                android:theme="@android:style/Theme.Holo.Light.NoActionBar.Overscan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:timePickerMode="spinner"
                android:layout_toRightOf="@id/datePickerStart"
                android:layout_alignTop="@id/datePickerStart"
                ></TimePicker>
        </com.zhy.autolayout.AutoLinearLayout>

    </com.zhy.autolayout.AutoRelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginLeft="30px"
        android:layout_marginRight="30px"
        android:layout_marginBottom="10px"
        android:background="#ccc"/>
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/end_title"
            android:textSize="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="结束日期" />
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/end_title"
            android:paddingLeft="27px"
            >
            <DatePicker
                android:theme="@android:style/Theme.Holo.Light.NoActionBar.Overscan"
                android:id="@+id/datePickerEnd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:calendarViewShown="false"
                android:layout_below="@+id/end_title"
                android:layout_centerHorizontal="true" />
            <TimePicker
                android:id="@+id/tp_end"
                android:theme="@android:style/Theme.Holo.Light.NoActionBar.Overscan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/datePickerEnd"
                android:layout_alignTop="@id/datePickerEnd"
                android:timePickerMode="spinner"
                ></TimePicker>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
    <View
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="1px"
        android:layout_marginLeft="30px"
        android:layout_marginRight="30px"
        android:background="#ccc"/>
    <Button
        android:id="@+id/print_total"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textSize="14sp"
        android:layout_marginLeft="30px"
        android:layout_marginRight="30px"
        android:background="@drawable/login_button_selector"
        android:text="打印数据"
        android:textColor="#fff"
        android:layout_marginTop="50px"
        android:layout_marginBottom="50px"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/pn_selector"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginBottom="20px"
        android:layout_marginTop="30px">
        <TextView
            android:id="@+id/tv_no"
            android:layout_width="175px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="取消"
            android:textSize="28px"
            android:background="@drawable/native_btn_selector"/>
        <TextView
            android:id="@+id/tv_yes"
            android:layout_width="175px"
            android:layout_height="match_parent"
            android:textColor="#fff"
            android:text="确定"
            android:background="@drawable/positive_btn_selector"
            android:gravity="center"
            android:textSize="28px"
            android:layout_marginLeft="35px"
            />
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>