<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    >
    <LinearLayout
        android:id="@+id/ll_regist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#ffffff">
        <include layout="@layout/titlebar_all"/>

        <EditText
            android:id="@+id/et_regist_mobilenum"
            android:layout_marginTop="15dp"
            android:paddingLeft="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/password_find_mobilenum"
            android:hint="手机号码"
            android:textSize="15sp"
            />
        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">
            <EditText
                android:id="@+id/et_regist_smscode"
                android:layout_width="160dp"
                android:layout_height="40dp"
                android:background="@drawable/password_find_mobilenum"
                android:hint="验证码"
                android:textSize="15sp"
                android:paddingLeft="5dp"/>
            <Button
                android:id="@+id/btn_regist_getcode"
                android:layout_marginLeft="5dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="获取验证码"
                android:textSize="15sp"
                android:background="@drawable/password_find_getcode_selector"
                android:textColor="#3F88CE"
                />
        </LinearLayout>
        <EditText
            android:id="@+id/et_regist_companyname"
            android:layout_marginTop="15dp"
            android:paddingLeft="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/password_find_mobilenum"
            android:hint="公司名称"
            android:textSize="15sp"
            />
        <EditText
            android:inputType="textPassword"
            android:id="@+id/et_regist_password"
            android:layout_marginTop="15dp"
            android:paddingLeft="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/password_find_mobilenum"
            android:hint="登录密码"
            android:textSize="15sp"
            />
        <Button
            android:id="@+id/btn_to_regist"
            android:layout_marginTop="30dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/login_button_selector"
            android:text="立即注册"
            android:textColor="#ffffff"
            android:textSize="15sp"/>
    </LinearLayout>



</FrameLayout>
