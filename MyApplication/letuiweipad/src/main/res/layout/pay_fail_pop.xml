<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="634px"
    android:minWidth="634px"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:background="@drawable/pup_shape">
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="106px"
        android:background="@drawable/pay_fail_title_shape"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="37dp"
            android:drawableLeft="@drawable/pay_fail"
            android:gravity="center"
            android:text="收款失败"
            android:textColor="#fff"
            android:drawablePadding="30px"
            android:textSize="36px"/>
    </com.zhy.autolayout.AutoRelativeLayout>
   <!-- <TextView
        android:id="@+id/tv_payment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="收款f：323元"
        android:maxEms="13"
        android:layout_marginTop="54px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>-->
    <TextView
        android:id="@+id/tv_payway"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="收款方式：现金记账"
        android:maxEms="13"
        android:layout_marginTop="54px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>
    <TextView
        android:id="@+id/order_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="商户单号：983759812375019"
        android:maxEms="13"
        android:layout_marginTop="10px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>
    <TextView
        android:id="@+id/deal_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="交易时间：2017-09-18 11:52"
        android:maxEms="13"
        android:layout_marginTop="10px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>
    <TextView
        android:id="@+id/tv_fail_reason"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="失败原因："
        android:maxEms="13"
        android:layout_marginTop="10px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>
    <TextView
        android:id="@+id/tv_fail_solution"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="处理方法："
        android:maxEms="13"
        android:layout_marginTop="10px"
        android:layout_marginLeft="108px"
        android:lineSpacingMultiplier="1.2"
        android:singleLine="false"
        android:textSize="28px"
        android:textColor="#333"
        android:gravity="left|bottom"
        android:layout_marginRight="108px"/>


    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:orientation="horizontal"
        android:gravity="left"
        android:layout_marginLeft="108px"
        android:layout_marginTop="55px"
        android:layout_marginBottom="55px"
        android:layout_marginRight="108px"
        >
         <TextView
            android:id="@+id/tv_yes"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:background="@drawable/native_btn_selector"
            android:gravity="center"
            android:text="返回重新收款"
            android:textColor="#333"
            android:textSize="28px" />
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>